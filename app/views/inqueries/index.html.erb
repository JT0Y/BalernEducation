<p id="notice"><%= notice %></p>

<h1>Listing Inqueries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inqueries.each do |inquery| %>
      <tr>
        <td><%= link_to 'Show', inquery %></td>
        <td><%= link_to 'Edit', edit_inquery_path(inquery) %></td>
        <td><%= link_to 'Destroy', inquery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inquery', new_inquery_path %>
